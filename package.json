{
  "name": "human-talks-graphql",
  "version": "1.0.0",
  "main": "src/server.js",
  "license": "MIT",
  "scripts": {
    "build": "parcel build src/ui.js --out-file dist/humantalks.min.js --global HumanTalks",
    "watch": "parcel watch src/ui.js --out-file dist/humantalks.min.js --global HumanTalks",
    "start": "nodemon src/server.js",
